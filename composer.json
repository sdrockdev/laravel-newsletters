{
    "name": "sdrockdev/laravel-newsletters",
    "description": "Manage newsletters in Laravel (for multiple accounts)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Turrietta",
            "email": "nick.turrietta@sdrock.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Sdrockdev\\Newsletters\\NewsletterFactoryServiceProvider"
            ],
            "aliases": {
                "Newsletters": "Sdrockdev\\Newsletters\\NewsletterFactoryFacade"
            }
        }
    },
    "require": {
        "php": "~7.0",
        "spatie/laravel-newsletter": "~4.0",
        "illuminate/support": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Sdrockdev\\Newsletters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sdrockdev\\Newsletters\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.3"
    }
}
